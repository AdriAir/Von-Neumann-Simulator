<SCRIPT language=3DJavaScript>
		// Funcionamiento de la m=E1quina
		var p =3D 0;
		var estado =3D 0;
		var paso =3D 0;
		var inst;

		function pulso()
		{
		=09
			p++;
			apagarTodo();
		=09
			// RDM <- CP
			if (estado =3D=3D 0) {
				signals.value =3D "Fase b=FAsqueda (Captar Instrucci=F3n):\n";
				signals.value +=3D "["+p+"] RDM <- CP\n";
				signals.value +=3D "Se=F1alRDM: Cargar desde CP\n";

				rdm.value =3D cp.value;
				iluminar("rdm");
				iluminar("m" + rdm.value);
			=09
				estado++;
			}
			// RIM <- M(RDM)
			else if (estado =3D=3D 1) {
				signals.value =3D "["+p+"] RIM <- M(RDM)\n";
				signals.value +=3D "se=F1alMem: Leer\n";
				signals.value +=3D "Se=F1alRIM: Cargar desde memoria\n";

				eval("rim.value =3D m"+ rdm.value +".value;");
				iluminar("rim");
			=09
				estado++;
			}
			// RI <- RIM
			else if (estado =3D=3D 2) {
				signals.value =3D "["+p+"] RI <- RIM\n";
				signals.value +=3D "Se=F1alRI: Cargar\n";

				ri.value =3D rim.value;
				iluminar("ri");

				inst =3D analizar(ri.value);
				codOp.value =3D inst.o;
				iluminar("codOp");
			=09
				if (inst.type =3D=3D 1) {
					ra.value =3D "";
					rb.value =3D "";
					m.value =3D inst.a.charAt(1);
					iluminar("m");
				}
				else if (inst.type =3D=3D 2) {
					ra.value =3D inst.a;
					rb.value =3D inst.b;
					m.value =3D "";
					iluminar("ra");
					iluminar("rb");
				}
				else {
					ra.value =3D "";
					rb.value =3D "";
					m.value =3D "";
				}
			=09
				estado++;
			}
			// Fase ejecuci=F3n
			else if (estado =3D=3D 3) {
				p--;
			=09
				if (paso =3D=3D 0)
					signals.value =3D "Fase de ejecuci=F3n:\n";
				=09
				if (inst.type =3D=3D 1) {
					if (inst.o =3D=3D "CAR") {
						// RDM <- m
						if (paso =3D=3D 0) {
							signals.value +=3D "["+p+", "+ (paso+1) +"] RDM <- m\n";
							signals.value +=3D "Se=F1alRDM: Cargar desde m\n";
						=09
							rdm.value =3D m.value;
							iluminar("rdm");
							paso++;
						}
						// RIM <- M(RDM)
						else if (paso =3D=3D 1) {
							signals.value =3D "["+p+", "+ (paso+1) +"] RIM <- M(RDM)\n";
							signals.value +=3D "Se=F1alMem: Leer\n";
							signals.value +=3D "Se=F1alRIM: Cargar desde memoria\n";
						=09
							eval("rim.value =3D m"+ rdm.value +".value;");
							iluminar("rim");
							paso++;
						}
						// r0 <- RIM
						else if (paso =3D=3D 2) {
							signals.value =3D "["+p+", "+ (paso+1) +"] r0 <- RIM\n";
							signals.value +=3D "Se=F1alRegistro: r0 <- RIM\n";
						=09
							r0.value =3D rim.value;
							iluminar("r0");
							paso =3D 0;
							estado ++;
						}
					}
					else if (inst.o =3D=3D "ALM") {
						// RDM <- m
						if (paso =3D=3D 0) {
							signals.value +=3D "["+p+", "+ (paso+1) +"] RDM <- m\n";
							signals.value +=3D "Se=F1alRDM: Cargar desde m\n";
						=09
							rdm.value =3D m.value;
							iluminar("rdm");
							paso++;
						}
						// RIM <- r0
						else if (paso =3D=3D 1) {
							signals.value =3D "["+p+", "+ (paso+1) +"] r0 <- RIM\n";
							signals.value +=3D "Se=F1alRIM: Cargar desde r0\n";
						=09
							rim.value =3D r0.value;
							iluminar("rim");
							paso++;
						}
						// M(RDM) <- RIM
						else if (paso =3D=3D 2) {
							signals.value =3D "["+p+", "+ (paso+1) +"] M(RDM) <- RIM\n";
							signals.value +=3D "Se=F1alMem: Escribir\n";
						=09
							eval("m"+ rdm.value +".value =3D rim.value;");
							iluminar("m"+ rdm.value);
						=09
							paso =3D 0;
							estado++;
						}
					}
					else if (inst.o =3D=3D "SAL"){
						signals.value +=3D "["+p+", "+ (paso+1) +"] CP <- m\n";
						signals.value +=3D "Se=F1alCP: Cargar desde m\n";
						signals.value +=3D "Fase de ejecuci=F3n finalizada:\n";
						signals.value +=3D "(Salto efectuado)\n";
					=09
						cp.value =3D m.value;
						iluminar("cp");
						estado =3D 0;
					}
					else if (inst.o =3D=3D "SIG"){
						signals.value +=3D "["+p+", "+ (paso+1) +"] Si ZF=3D1 entonces CP =
<- m\n";

						if (zf.value =3D=3D "1") {
							signals.value +=3D "Se=F1alCP: Cargar desde m\n";
							signals.value +=3D "Fase de ejecuci=F3n finalizada:\n";
							signals.value +=3D "(Salto efectuado)\n";

							cp.value =3D m.value;
							iluminar("cp");
							estado =3D 0;
						}
						else {
							signals.value +=3D "(No hay se=F1ales)\n";
							signals.value +=3D "(Salto no efectuado)\n";
							estado++;
						}
					}
					else if (inst.o =3D=3D "SMA"){
						signals.value +=3D "["+p+", "+ (paso+1) +"] Si ZF=3D0 y SF=3D0 =
entonces CP <- m\n";

						if ((zf.value =3D=3D "0") && (sf.value =3D=3D "0")) {
							signals.value +=3D "Se=F1alCP: Cargar desde m\n";
							signals.value +=3D "Fase de ejecuci=F3n finalizada:\n";
							signals.value +=3D "(Salto efectuado)\n";

							cp.value =3D m.value;
							iluminar("cp");
							estado =3D 0;
						}
						else {
							signals.value +=3D "(No hay se=F1ales)\n";
							signals.value +=3D "(Salto no efectuado)\n";
							estado++;
						}
					}
					else if (inst.o =3D=3D "SME"){
						signals.value +=3D "["+p+", "+ (paso+1) +"] Si SF !=3D OF entonces =
CP <- m\n";

						if (of.value !=3D sf.value) {
							signals.value +=3D "Se=F1alCP: Cargar desde m\n";
							signals.value +=3D "Fase de ejecuci=F3n finalizada:\n";
							signals.value +=3D "(Salto efectuado)\n";

							cp.value =3D m.value;
							iluminar("cp");
							estado =3D 0;
						}
						else {
							signals.value +=3D "(No hay se=F1ales)\n";
							signals.value +=3D "(Salto no efectuado)\n";
							estado++;
						}
					}
				}
				else if (inst.type =3D=3D 2) {
					if (inst.o =3D=3D "COP") {
						signals.value +=3D "["+p+", "+ (paso+1) +"] "+ inst.b +" <- "+ =
inst.a +"\n";
						signals.value +=3D "Se=F1alRa: Seleccionar "+ inst.a +" (Op1 =3D =
"+ inst.a +")\n";
						signals.value +=3D "Se=F1alALU: Transferir (Result. =3D Op1)\n";
						signals.value +=3D "Se=F1alRegistro: "+ inst.b +" <- Resultado\n";
					=09
						eval(inst.b +".value =3D "+ inst.a +".value;");
						iluminar(inst.b);
					}
					else if (inst.o =3D=3D "CMP") {
						signals.value +=3D "["+p+", "+ (paso+1) +"] Comparar "+ inst.a +" =
con "+ inst.b +"\n";
						signals.value +=3D "Se=F1alRa: Seleccionar "+ inst.a +" (Op1 =3D =
"+ inst.a +")\n";
						signals.value +=3D "Se=F1alRb: Seleccionar "+ inst.b +" (Op2 =3D =
"+ inst.b +")\n";
						signals.value +=3D "Se=F1alALU: Comparar\n";
					=09
						eval("alu("+ inst.a +".value, "+ inst.b +".value, 'C');");
					}
					else {
						var oper =3D new Array();
						oper["SUM"] =3D "+";
						oper["RES"] =3D "-";
						oper["MUL"] =3D "*";
						oper["DIV"] =3D "/";

						var operS =3D new Array();
						operS["SUM"] =3D "Sumar";
						operS["RES"] =3D "Restar";
						operS["MUL"] =3D "Multiplicar";
						operS["DIV"] =3D "Dividir";
					=09
						signals.value +=3D "["+p+", "+ (paso+1) +"] r0 <- "+ inst.a +" "+ =
oper[inst.o] +" "+ inst.b +"\n";
						signals.value +=3D "Se=F1alRa: Seleccionar "+ inst.a +" (Op1 =3D =
"+ inst.a +")\n";
						signals.value +=3D "Se=F1alRb: Seleccionar "+ inst.b +" (Op2 =3D =
"+ inst.b +")\n";
						signals.value +=3D "Se=F1alALU: "+ operS[inst.o] +"\n";
						signals.value +=3D "Se=F1alRegistro: r0 <- Resultado\n";
					=09
						eval("alu("+ inst.a +".value, "+ inst.b +".value, '"+ =
inst.o.charAt(0) +"');");
					}

					paso =3D 0;
					estado ++;
				}
				else if (inst.type =3D=3D 3) {
					if (inst.o =3D=3D "NOP") {
						signals.value +=3D "["+p+"] (No hay se=F1ales)\n";
				=09
						estado++;
					}
				}
			}
			// CP <- CP + 1
			else if (estado =3D=3D 4) {
				if (cp.value =3D=3D "F") {
